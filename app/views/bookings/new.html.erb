<h1>New Booking</h1>

# Create form to create a new booking
<%= form_with(model: @booking) do |form| %>
  <%= form.label :seat_id %>
  <%= form.text_field :seat_id %>
  <%= form.label :from_date %>
  <%= form.text_field :from_date %>
  <%= form.label :to_date %>
  <%= form.text_field :to_date %>
  <%= form.label :check_in %>
  <%= form.text_field :check_in %>
  <%= form.submit %>
<% end %>

<%= link_to 'Back', bookings_path %>